{
    "name": "Name instead of UserId for GE GitHub",
    "description": "Display full name instead of SSO for GE GitHub Enterprise instance",
    "version": "0.4.0",
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "index.bundle.js"
        ],
        "matches": [
            "https://github.build.ge.com/*",
            "https://another.domain.here/*"
        ],
        "exclude_matches": [
        	"https://github.build.ge.com/pages*"
        ]
    }],
    "permissions": [
    	"storage"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
